<Window x:Class="JarvisEmulator.UserInterface.CreateUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JarvisEmulator.UserInterface"
        mc:Ignorable="d"
        Title="CreateUser" Height="300" Width="300">
    <StackPanel>
        <Label Content="Create User"
               HorizontalAlignment="Center"
               FontSize="16" />

        <Label Content="First Name:"
               HorizontalAlignment="Left" />
        <TextBox Name="tboxUserName"
                 Text="{Binding CommandKey, Mode=TwoWay}"
                 Margin="0, 0, 0, 20" />
    </StackPanel>
</Window>
